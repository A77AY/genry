{
    "name": "@genry/vsc-extension",
    "displayName": "Genry Scaffolder",
    "description": "Genry VS Code Scaffolder Extension",
    "keywords": [
        "scaffolder",
        "generator"
    ],
    "version": "0.1.0",
    "license": "MIT",
    "author": {
        "name": "Rinat Arsaev",
        "email": "krickray@gmail.com"
    },
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:genry.create",
        "onCommand:genry.createByMenu"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "genry.create",
                "title": "genry: Create by template"
            },
            {
                "command": "genry.createByMenu",
                "title": "Scaffold by template"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "genry.createByMenu"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "yarn clear && tsc -watch -p ./",
        "clear": "shx rm -rf out",
        "pretest": "yarn compile && yarn lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/mocha": "^7.0.2",
        "@types/node": "^12.12.35",
        "@types/vscode": "^1.44.0",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "eslint": "^6.8.0",
        "mocha": "^7.1.1",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "@types/glob": "^7.1.1",
        "@types/node-ipc": "^9.1.2",
        "@types/uuid": "^7.0.2",
        "glob": "^7.1.6",
        "node-ipc": "^9.1.1",
        "uuid": "^7.0.3"
    }
}
